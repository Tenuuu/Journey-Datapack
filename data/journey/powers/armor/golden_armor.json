{
    "type": "origins:multiple",
    "hidden": true,

    "set": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:area_of_effect",
            "radius": 6,
            "shape": "sphere",
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:apply_effect",
                    "effect": {
                        "id": "journey:melee_speed_aura",
                        "duration": 10,
                        "amplifier": 0,
                        "show_particles": false
                    }
                }
            },
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:player"
                }
            }
        },
        "condition": {
            "type": "origins:all_of",
            "conditions": [
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "head",
                    "item_condition": {
                        "type": "origins:ingredient",
                        "ingredient":
                            {
                                "item": "minecraft:golden_helmet"
                            }
                    }
                },
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "chest",
                    "item_condition": {
                        "type": "origins:ingredient",
                        "ingredient":
                            {
                                "item": "minecraft:golden_chestplate"
                            }
                    }
                },
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "legs",
                    "item_condition": {
                        "type": "origins:ingredient",
                        "ingredient":
                            {
                                "item": "minecraft:golden_leggings"
                            }
                    }
                },
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "feet",
                    "item_condition": {
                        "type": "origins:ingredient",
                        "ingredient":
                            {
                                "item": "minecraft:golden_boots"
                            }
                    }
                }
            ]
        }
    },

    "helmet": {
        "type": "origins:action_over_time",
        "hidden": true,
        "rising_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.head",
            "stack": {
                "id": "minecraft:golden_helmet"
            }
        },
        "falling_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.head",
            "stack": {
                "id": "minecraft:air"
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "chest",
            "item_condition": {
                "type": "origins:ingredient",
                "ingredient": {
                    "item": "minecraft:golden_chestplate"
                }
            }
        }
    },

    "leggings": {
        "type": "origins:action_over_time",
        "hidden": true,
        "rising_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.legs",
            "stack": {
                "id": "minecraft:golden_leggings"
            }
        },
        "falling_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.legs",
            "stack": {
                "id": "minecraft:air"
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "chest",
            "item_condition": {
                "type": "origins:ingredient",
                "ingredient": {
                    "item": "minecraft:golden_chestplate"
                }
            }
        }
    },

    "boots": {
        "type": "origins:action_over_time",
        "hidden": true,
        "rising_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.feet",
            "stack": {
                "id": "minecraft:golden_boots"
            }
        },
        "falling_action": {
            "type": "origins:replace_inventory",
            "slot": "armor.feet",
            "stack": {
                "id": "minecraft:air"
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "chest",
            "item_condition": {
                "type": "origins:ingredient",
                "ingredient": {
                    "item": "minecraft:golden_chestplate"
                }
            }
        }
    }
}