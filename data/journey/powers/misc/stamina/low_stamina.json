{
    "type": "origins:multiple",
    "hidden": true,
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "hud_render": {
            "should_render": false
        }
    },

    "prevent_sprint": {
        "type": "origins:prevent_sprinting",
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "compare_to": 1,
            "comparison": "=="
        }
    },

    "prevent_shield": {
        "type": "origins:prevent_item_use",
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "compare_to": 1,
            "comparison": "=="
        },
        "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
                "item": "minecraft:shield"
            }
        }
    },

    "prevent_jump": {
        "type": "origins:modify_jump",
        "modifier": {
            "operation": "set_total",
            "amount": 0
        },
        "entity_action": {
            "type": "origins:add_velocity",
            "set": true
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "compare_to": 1,
            "comparison": "=="
        }
    },
    
    "speed": {
        "type": "origins:modify_attribute",
        "attribute": "minecraft:generic.movement_speed",
        "modifier": {
            "operation": "multiply_base_additive",
            "amount": -0.2
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "compare_to": 1,
            "comparison": "=="
        }
    }
}